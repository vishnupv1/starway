{
	"version": 2,
	"builds": [
		{
			"src": "bexon/package.json",
			"use": "@vercel/next",
			"config": {
				"distDir": ".next"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "bexon/$1"
		}
	],
	"outputDirectory": "bexon/.next",
	"framework": "nextjs",
	"installCommand": "cd bexon && npm install",
	"buildCommand": "cd bexon && npm run build",
	"devCommand": "cd bexon && npm run dev",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/bexon/$1"
		}
	]
}
